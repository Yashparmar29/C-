#include<iostream>
using namespace std;

int main()
{
    int arr[5];

    for(int i=0;i<5;i++)
    {
        cout << "Enter no :- ";
        cin >> arr[i];
    }
    for(int i=0;i<5;i++)
    {
        int x = i;
        for(int j=i+1;j<5;j++)
        {
            if(arr[j] < arr[x])
            {
                x = j;
            }
        }

        int temp = arr[i];
        arr[i] = arr[x];
        arr[x] = temp;
    }

    cout << "-- sorted arrya --";
    for(int i=0;i<5;i++)
    {
        cout << arr[i] << ",";
    }

}

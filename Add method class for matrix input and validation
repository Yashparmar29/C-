#include<iostream>
using namespace std;

class method
{
    int n,m;

    public :
        method()
        {
            cout << "Enter n no :- ";
            cin >> n;
            cout << "Enter m no :- ";
            cin >> m;
            int arr[n][m];
            if(n == m)
            {
                for(int i=0;i<n;i++)
                {
                    for(int j=0;j<m;j++)
                    {
                        cout << "Enter arr[n]"<<m<< "no :- ";
                        cin >> arr[i][j];
                    }
                }
                for(int i=0;i<n;i++)
                {
                    for(int j=0;j<m;j++)
                    {
                        cout << arr[i][j] << " ";
                    }
                    cout << endl;
                }
            } 
            else
            {
                cout << "Array is not possible";
            }       
        }
};

int main()
{
    method m;
}

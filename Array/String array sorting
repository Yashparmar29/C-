#include<iostream>
#include<string>
using namespace std;

class method
{
    string name[5];
    string *n[5];
    public :
        method()
        {
            for(int i=0;i<5;i++)
            {
                cout << "Enter name :- ";
                cin >> name[i];
                
            }
            for(int i=0;i<5;i++)
            {
                for(int j=0;j<i;j++)
                {
                    if(name[i] < name[j])
                    {
                        string n = name[i];
                        name[i] = name[j];
                        name[j] = n;
                    }
                }
            }
            for(int i=0;i<5;i++)
            {
                cout << name[i] << endl;

            }
        }
};

int main()
{
    method m;
}

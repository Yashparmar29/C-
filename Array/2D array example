#include <iostream>
using namespace std;

int main() 
{
    int rows = 3;
    int cols = 3;
    int grid[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    cout << "Displaying the 3x3 Grid:" << endl;
    
    for (int i = 0; i < rows; i++) 
    {               
            for (int j = 0; j < cols; j++) 
            {     
              cout << grid[i][j] << " ";
            }
            cout << endl; // New line after each row
    }

    return 0;
}
